---
import type { GetStaticPathsOptions } from 'astro';

import Page, {
  generateStaticPaths,
} from '@/pages/fr/blog/[...filter]/[...page]/index.astro';

export async function getStaticPaths(params: GetStaticPathsOptions) {
  return generateStaticPaths({ ...params, locale: 'en' });
}
---

<Page {...Astro.props} />
